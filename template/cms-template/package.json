{
  "name": "<%=name %>",
  "version": "1.0.0",
  "description": "<%=description %>",
  "scripts": {
    "precommit": "npm run test",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "clean": "rimraf ./dist/ && mkdirp ./dist/",
    "start": "npm run clean && webpack-dev-server",
    "analyzer": "npm run clean && webpack --env.analyzer --progress --profile --json > ./dist/stats.json && webpack-bundle-analyzer ./dist/stats.json -m static -r ./dist/report.html",
    "test": "npm run lint && npm run lint:style",
    "lint": "eslint \"src/**/*.+(js|ts|tsx|jsx)\" --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:ts": "eslint \"src/**/*.+(ts|tsx)\" --fix",
    "typecheck": "tsc --noEmit",
    "typecheck--watch": "npm run typecheck -- --watch",
    "build": "npm run clean && webpack --verbose --env.prod",
    "build:prod": "npm run clean && webpack --verbose --env.prod",
    "build:test": "npm run clean && webpack --verbose --env.test"
  },
  "keywords": [
    "react",
    "react-cms"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
