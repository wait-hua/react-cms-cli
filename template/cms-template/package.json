{
    "name": "{{name}}",
    "version": "1.0.0",
    "description": "{{description}}",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf ./dist/ && mkdirp ./dist/",
        "start": "npm run clean && webpack-dev-server",
        "analyzer": "npm run clean && webpack --env.analyzer --progress --profile --json > ./dist/stats.json && webpack-bundle-analyzer ./dist/stats.json -m static -r ./dist/report.html",
        "test": "npm run lint",
        "lint": "elint lint \"src/**/*.+(js|jsx|scss)\" \"build/**/*.js\" --fix",
        "beforecommit": "npm run test && elint lint commit",
        "build": "npm run clean && webpack",
        "build:prod": "npm run clean && webpack --verbose --env.prod",
        "build:test": "npm run clean && webpack --verbose --env.test"
    },
    "keywords": [
        "monitor"
    ],
    "author": "{{author}}",
    "license": "ISC",
